<h3>Closet Item <%= @closet.id %> updated</h3>

<p>Store: <%= @closet.store %></p>

<p>Category: <%= @closet.category %></p>

<p>Style: <%= @closet.style %></p>

<p>Size: <%= @closet.size %></p>

<p>Notes: <%= @closet.note %></p>

<div class="links">
<%= link_to 'Edit', edit_closet_path(@closet) %> |
<%= link_to 'Delete', @closet, method: :delete, data: { confirm: 'Are you sure?' } %> |
<%= link_to 'Home', root_path %>
</div>
